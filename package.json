    "mkdirp": "^0.5.1",
    "shortid": "^2.2.8",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-lodash": "^2.0.2",
    "vue-router": "^3.0.3",
